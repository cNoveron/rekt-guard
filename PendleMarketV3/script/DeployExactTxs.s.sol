// Deploy.s.sol
pragma solidity ^0.8.0;

import "forge-std/Script.sol";

contract DeployScript is Script {
    function run() external {
        vm.startBroadcast();

        // Your creation bytecode from the original tx
        bytes memory creationCode = ""; // full bytecode here

        address deployedContract;
        assembly {
            deployedContract := create(0, add(creationCode, 0x20), mload(creationCode))
        }

        console.log("Contract deployed at:", deployedContract);
        vm.stopBroadcast();
    }
}